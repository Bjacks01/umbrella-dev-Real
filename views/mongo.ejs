<h1>see my mongoDB results</h1>

<h1> insert something into mongo</h1>
<form method="POST" action="/insert" style="display: inline-block">  
  <input id="newPost" name="newPost">
  <input id="teamName" name="teamName"> 
  <input type="submit" value="update"> 
</form>

<h3> my blog posts/team names</h3> 
  <% for (var i=0; i< mongoResult.length; i++ ) { %>
    <form method="POST" action="/update/<%= mongoResult[i]._id %>" style="display: inline-block">  
      <%= mongoResult[i].teamName %> 
    <input type="submit" value="update"> 
  </form>
  <form method="POST" action="/delete/<%= mongoResult[i]._id %>" style="display: inline-block">  
    <input type="submit" value="delete"> 
  </form>
  <br/>
<% } %> 

     
